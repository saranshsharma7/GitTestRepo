public void open_chrome_browser() {
     System.setProperty("webdriver.chrome.driver",System.getProperty("user.dir")+"./Drivers/chromedriver.exe");
     driver=new ChromeDriver();
     db=new pageObjectBlaze(driver);
}

@When("Open Website URL: {string}")
public void open_website_url(String WebsiteURL) {
     driver.get(WebsiteURL);
}

@Then("Website Home page should be Open URL: {string}")
public void website_home_page_should_be_open_url(String ExpectedWebsiteURL) {
    
        if(driver.getCurrentUrl().equals(ExpectedWebsiteURL))
        {
            Assert.assertTrue(true);
        }
        
        else {
            Assert.assertTrue(false);
        }
        }

@Then("Login pop-up box will Open")
public void login_pop_up_box_will_open() {
     db.clickLoginButton();
       
       driver.switchTo().alert();
       driver.switchTo().alert().dismiss();
     
     
       
       driver.quit();
}


